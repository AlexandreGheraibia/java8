import java.lang.*; // headers MUST be above the first class
import java.util.*;
import java.util.stream.*;
import java.util.function.*;
import static java.util.Arrays.*;
import static java.lang.System.*;

// one class needs to have a main() method
public class HelloWorld
{
 // arguments are passed using the text field below this editor
  public static void main(String[] args)
  {
     List<Integer> intList=asList(12,25,17,20,19,8,3);
     intList.stream().map(p1-> intList.parallelStream().map(p2->{return p1+p2==20 ? Optional.of(asList(p1,p2)):Optional.empty();}))
                     .flatMap(stream -> stream.filter(Optional::isPresent))
            		 .findAny()
            		 .ifPresent(e->System.out.println(e.get()));
          		     		
    	
  }
}
